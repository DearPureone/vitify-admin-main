{
  "private": true,
  "type": "module",
  "packageManager": "pnpm@8.15.3",
  "scripts": {
    "dev": "vite --open --host",
    "build": "vite build",
    "preview": "vite preview --port 5050 --host --config vite.config.preview.ts",
    "test:e2e": "start-server-and-test preview http://127.0.0.1:5050/ 'cypress open'",
    "test:e2e:ci": "start-server-and-test preview http://127.0.0.1:5050/ 'cypress run'",
    "test:unit": "vitest",
    "coverage": "vitest run --coverage",
    "typecheck": "vue-tsc --noEmit -p tsconfig.app.json --composite false",
    "lint": "eslint . --ext .vue,.js,.jsx,.cjs,.mjs,.ts,.tsx,.cts,.mts --fix --ignore-path .gitignore"
  },
  "dependencies": {
    "@mdi/js": "^7.4.47",
    "@vueuse/core": "^10.7.2",
    "axios": "^1.6.7",
    "echarts": "^5.5.0",
    "pinia": "^2.1.7",
    "portal-vue": "^2.1.7",
    "vue": "^2.7.16",
    "vue-echarts": "^6.6.8",
    "vue-i18n": "^8.28.2",
    "vue-i18n-bridge": "^9.9.1",
    "vue-router": "^3.6.5",
    "vuetify": "^2.7.2"
  },
  "devDependencies": {
    "@intlify/unplugin-vue-i18n": "^2.0.0",
    "@kingyue/vite-plugin-vue2-svg": "^0.6.0",
    "@pinia/testing": "^0.1.3",
    "@rushstack/eslint-patch": "^1.7.2",
    "@testing-library/vue": "^5.9.0",
    "@types/node": "^20.11.19",
    "@vitejs/plugin-legacy": "^5.3.0",
    "@vitejs/plugin-vue2": "^2.3.1",
    "@vue/eslint-config-prettier": "^9.0.0",
    "@vue/eslint-config-typescript": "^12.0.0",
    "@vue/test-utils": "^1.3.6",
    "browserslist-to-esbuild": "^2.1.1",
    "cypress": "^13.6.4",
    "eslint": "^8.56.0",
    "eslint-plugin-cypress": "^2.15.1",
    "eslint-plugin-vue": "^9.21.1",
    "flush-promises": "^1.0.2",
    "jsdom": "^24.0.0",
    "msw": "^2.2.1",
    "postcss-preset-env": "^9.3.0",
    "prettier": "^3.2.5",
    "rollup-plugin-regexp": "^5.0.1",
    "sass": "~1.32.13",
    "start-server-and-test": "^2.0.3",
    "terser": "^5.27.1",
    "typescript": "^5.3.3",
    "unplugin-auto-import": "^0.17.5",
    "unplugin-vue-components": "^0.26.0",
    "vite": "^5.1.3",
    "vite-plugin-inspect": "^0.8.3",
    "vite-plugin-pages": "^0.32.0",
    "vite-plugin-vue-layouts": "^0.8.0",
    "vitest": "^1.3.0",
    "volar-plugin-vue-router": "^0.2.0",
    "vue-template-compiler": "^2.7.16",
    "vue-tsc": "^1.8.27",
    "vuetify2-component-types": "^2.7.2"
  },
  "browserslist": [
    "> 1.3%",
    "last 2 versions",
    "not dead",
    "not op_mini all",
    "not ie>0"
  ],
  "msw": {
    "workerDirectory": "public"
  },
  "pnpm": {
    "peerDependencyRules": {
      "allowedVersions": {
        "vite-plugin-vue-layouts>vite": "5"
      }
    },
    "allowedDeprecatedVersions": {
      "vue": "2"
    }
  }
}
