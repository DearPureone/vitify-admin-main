{
  "private": true,
  "type": "module",
  "packageManager": "pnpm@8.12.1",
  "scripts": {
    "dev": "vite --open --host",
    "build": "vite build",
    "preview": "vite preview --port 5050 --host --config vite.config.preview.ts",
    "test:e2e": "start-server-and-test preview http://127.0.0.1:5050/ 'cypress open'",
    "test:e2e:ci": "start-server-and-test preview http://127.0.0.1:5050/ 'cypress run'",
    "test:unit": "vitest",
    "coverage": "vitest run --coverage",
    "typecheck": "vue-tsc --noEmit -p tsconfig.app.json --composite false",
    "lint": "eslint . --ext .vue,.js,.jsx,.cjs,.mjs,.ts,.tsx,.cts,.mts --fix --ignore-path .gitignore"
  },
  "dependencies": {
    "@mdi/js": "^7.3.67",
    "@vueuse/core": "^10.7.0",
    "axios": "^1.6.2",
    "echarts": "^5.4.3",
    "pinia": "^2.1.7",
    "portal-vue": "^2.1.7",
    "vue": "^2.7.15",
    "vue-echarts": "6.6.1",
    "vue-i18n": "^8.28.2",
    "vue-i18n-bridge": "^9.8.0",
    "vue-router": "^3.6.5",
    "vuetify": "^2.7.1"
  },
  "devDependencies": {
    "@intlify/unplugin-vue-i18n": "^1.6.0",
    "@kingyue/vite-plugin-vue2-svg": "^0.6.0",
    "@kingyue/vuetify2-component-types": "^2.7.0",
    "@pinia/testing": "^0.1.3",
    "@rushstack/eslint-patch": "^1.6.1",
    "@testing-library/vue": "^5.9.0",
    "@types/node": "^20.10.4",
    "@vitejs/plugin-legacy": "^5.2.0",
    "@vitejs/plugin-vue2": "^2.3.1",
    "@vue/eslint-config-prettier": "^8.0.0",
    "@vue/eslint-config-typescript": "^12.0.0",
    "@vue/test-utils": "^1.3.6",
    "browserslist-to-esbuild": "^1.2.0",
    "cypress": "^13.6.1",
    "eslint": "^8.55.0",
    "eslint-plugin-cypress": "^2.15.1",
    "eslint-plugin-vue": "^9.19.2",
    "flush-promises": "^1.0.2",
    "jsdom": "^23.0.1",
    "msw": "^2.0.11",
    "postcss-preset-env": "^9.3.0",
    "prettier": "^3.1.1",
    "rollup-plugin-regexp": "^5.0.1",
    "sass": "~1.32.13",
    "start-server-and-test": "^2.0.3",
    "terser": "^5.26.0",
    "typescript": "^5.3.3",
    "unplugin-auto-import": "^0.17.2",
    "unplugin-vue-components": "^0.26.0",
    "vite": "^5.0.9",
    "vite-plugin-inspect": "^0.8.1",
    "vite-plugin-pages": "^0.32.0",
    "vite-plugin-vue-layouts": "^0.8.0",
    "vitest": "^1.0.4",
    "volar-plugin-vue-router": "^0.2.0",
    "vue-template-compiler": "^2.7.15",
    "vue-tsc": "^1.8.25"
  },
  "browserslist": [
    "> 1.3%",
    "last 2 versions",
    "not dead",
    "not op_mini all",
    "not ie>0"
  ],
  "msw": {
    "workerDirectory": "public"
  },
  "pnpm": {
    "peerDependencyRules": {
      "allowedVersions": {
        "vite": "5",
        "typescript":"^5.3.0"
      }
    }
  }
}
