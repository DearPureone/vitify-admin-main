{
  "name": "vitify-admin",
  "version": "0.0.1",
  "packageManager": "pnpm@7.2.1",
  "scripts": {
    "dev": "vite",
    "build": "vite build",
    "preview": "vite preview --port 5050",
    "test:e2e": "start-server-and-test preview http://127.0.0.1:5050/ 'cypress open'",
    "test:e2e:ci": "start-server-and-test preview http://127.0.0.1:5050/ 'cypress run'",
    "test:unit": "vitest",
    "coverage": "vitest run --coverage",
    "typecheck": "vue-tsc --noEmit -p tsconfig.app.json --composite false",
    "lint": "eslint . --ext .vue,.js,.jsx,.cjs,.mjs,.ts,.tsx,.cts,.mts --fix --ignore-path .gitignore",
    "supportedBrowsers": "(echo export default && browserslist-useragent-regexp --allowHigherVersions) > ./src/supportedBrowsers.ts",
    "type:vuetify": "node ./scripts/vuetify-type.mjs > ./src/vuetify2.d.ts",
    "postinstall": "patch-package"
  },
  "dependencies": {
    "@mdi/js": "^6.7.96",
    "@vue/composition-api": "^1.6.2",
    "@vueuse/core": "^8.6.0",
    "axios": "^0.27.2",
    "echarts": "^5.3.3",
    "js-cookie": "^3.0.1",
    "lodash": "^4.17.21",
    "nprogress": "^0.2.0",
    "pinia": "^2.0.14",
    "screenfull": "^6.0.1",
    "vue": "^2.6.14",
    "vue-echarts": "6.0.3",
    "vue-i18n": "^8.27.1",
    "vue-router": "^3.5.4",
    "vue2-helpers": "^1.1.7",
    "vuetify": "^2.6.6"
  },
  "devDependencies": {
    "@cypress/vite-dev-server": "^2.2.3",
    "@cypress/vue": "^2.2.4",
    "@milahu/patch-package": "^6.4.14",
    "@pinia/testing": "^0.0.12",
    "@rushstack/eslint-patch": "^1.1.3",
    "@testing-library/vue": "^5.8.3",
    "@types/js-cookie": "^3.0.2",
    "@types/lodash": "^4.14.182",
    "@types/node": "^16.11.40",
    "@types/nprogress": "^0.2.0",
    "@vitejs/plugin-legacy": "^1.8.2",
    "@vue/eslint-config-prettier": "^7.0.0",
    "@vue/eslint-config-typescript": "^11.0.0",
    "@vue/runtime-dom": "^3.2.37",
    "@vue/test-utils": "^1.3.0",
    "browserslist-useragent-regexp": "^3.0.2",
    "cypress": "^9.7.0",
    "eslint": "^8.17.0",
    "eslint-plugin-cypress": "^2.12.1",
    "eslint-plugin-vue": "^9.1.1",
    "flush-promises": "^1.0.2",
    "jsdom": "^19.0.0",
    "prettier": "^2.7.0",
    "sass": "~1.32.13",
    "start-server-and-test": "^1.14.0",
    "typescript": "^4.7.3",
    "unplugin-auto-import": "^0.8.8",
    "unplugin-vue-components": "^0.19.6",
    "unplugin-vue2-script-setup": "^0.11.0",
    "vite": "^2.9.12",
    "vite-plugin-inspect": "^0.5.0",
    "vite-plugin-vue2": "^2.0.1",
    "vite-plugin-vue2-svg": "^0.1.8",
    "vitest": "^0.15.1",
    "vue-template-babel-compiler": "^1.2.0",
    "vue-template-compiler": "^2.6.14",
    "vue-tsc": "^0.37.8"
  },
  "browserslist": [
    "> 1%",
    "last 2 versions",
    "not dead",
    "not op_mini all",
    "not ie>0"
  ]
}

